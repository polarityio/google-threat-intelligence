                <table>
                  <tbody>
                    {{#each details.behaviorSummary.files_opened as |file|}}
                      <tr>
                        <td>
                          {{file}}
                        </td>
                      </tr>
                    {{/each}}
                  </tbody>
                </table>
              </div>
            {{else}}
              <span>
                No opened files information available
              </span>
            {{/if}}
          {{/if}}
        </div>
      {{/if}}
      {{! end of behavior summary}}

      {{! start of relations}}
