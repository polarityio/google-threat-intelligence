                              {{else}}
                                {{#if (eq negativeScan.result "Suspicious")}}
                                  {{fa-icon "info" class="p-orange table-icon" fixedWidth=true}}
                                {{else}}
                                  {{fa-icon "check" class="p-green table-icon" fixedWidth=true}}
